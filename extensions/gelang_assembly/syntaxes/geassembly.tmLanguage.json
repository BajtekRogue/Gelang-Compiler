{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "GeAssembly",
	"patterns": [
	  {
		"include": "#keywords"
	  },
	  {
		"include": "#numbers"
	  },
	  {
		"include": "#comments"
	  }
	],
	"repository": {
	  "keywords": {
		"patterns": [
		  {
			"name": "keyword.control.geassembly",
			"match": "\\b(GET|PUT|LOAD|STORE|LOADI|STOREI|ADD|SUB|ADDI|SUBI|SET|HALF|JUMP|JPOS|JZERO|JNEG|RTRN|HALT)\\b"
		  }
		]
	  },
	  "numbers": {
		"patterns": [
		  {
			"name": "constant.numeric.geassembly",
			"match": "\\b\\d+\\b"
		  }
		]
	  },
	  "comments": {
		"patterns": [
		  {
			"name": "comment.line.geassembly",
			"match": "#.*$"
		  }
		]
	  }
	},
	"scopeName": "source.geassembly"
  }
  